job('New DSL Jobn') {
    scm {
        git('https://github.com/singhkarandx/JenkinsTest.git') {  node -> // is hudson.plugins.git.GitSCM
            node / gitConfigName('karan')
            node / gitConfigEmail('singhkarandx@gmail.com')
        }
    }
   
    steps {
        shell('chmod +x $WORKSPACE/count.sh
        bash count.sh')
        
    }
}
